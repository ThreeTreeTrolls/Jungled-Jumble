<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Auth.SelectUserActivity">

    <ImageView
        android:id="@+id/who"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="fitXY"
        android:src="@drawable/user_select_background"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="18dp" />

    <ImageView
        android:id="@+id/existing_user"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_marginStart="180dp"
        android:layout_marginTop="196dp"
        android:src="@drawable/user_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <ImageView
        android:id="@+id/new_user"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="104dp"
        android:background="@drawable/guest_default_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <ImageView
        android:id="@+id/new_user_active"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="104dp"
        android:background="@drawable/guest_selected_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/existing_user_active"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_marginStart="180dp"
        android:layout_marginTop="196dp"
        android:src="@drawable/user_selected"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />



    <ImageView
        android:id="@+id/add_user"
        android:layout_width="104dp"
        android:layout_height="95dp"
        android:src="@drawable/buttons_new_user"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.993"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/orange"
        android:layout_width="@dimen/fruit_height"
        android:layout_height="@dimen/fruit_height"
        android:src="@drawable/orange_detailed"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.836"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.898" />

    <ImageView
